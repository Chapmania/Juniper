<Project Sdk="Microsoft.NET.Sdk">

  <ImportGroup>
    <Import Project="..\..\Juniper48.targets" />
  </ImportGroup>

  <PropertyGroup>
    <AssemblyName>Juniper.NeuroSky.Console</AssemblyName>
    <RootNamespace>Juniper.NeuroSky</RootNamespace>
    <OutputType>Exe</OutputType>
    <StartupObject>Juniper.NeuroSky.Program</StartupObject>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>


    <Authors>Sean T. McBeth</Authors>
    <Copyright>Copyright Â© Sean T. McBeth 2019</Copyright>
    <StartupObject>Juniper.NeuroSky.Program</StartupObject>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Platform)' == 'x64'">
    <DefineConstants>WIN64</DefineConstants>
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Platform)' == 'x86'">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x64'">
    <OutputPath>bin\x64\Debug\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|x64'">
    <OutputPath>bin\x64\Release\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x86'">
    <OutputPath>bin\x86\Debug\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|x86'">
    <OutputPath>bin\x86\Release\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Juniper.NeuroSky\Juniper.NeuroSky.csproj" />
  </ItemGroup>

</Project>